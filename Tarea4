import java.util.*;
class Main {  
  public static void main(String args[]) { 
    List<Float> list=new ArrayList<Float>();
    list.add(2.5f);  
    list.add(new Float(8));  
    list.add(new Float(5));  
    list.add(new Float(6));  
 
    for(Float r:list){
      System.out.println(r);
    }
  } 
}
